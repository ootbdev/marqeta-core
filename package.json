{
  "name": "marqeta-core",
  "version": "1.0.0",
  "description": "Server-side Javascript client for the Marqeta Core API",
  "main": "index.js",
  "keywords": [
    "marqeta",
    "payment processing",
    "fintech",
    "credit cards",
    "virtual cards",
    "api"
  ],
  "homepage": "https://github.com/ootbdev/marqeta-core",
  "bugs": "https://github.com/ootbdev/marqeta-core/isses",
  "contributors": [
    "Alvin Lee <alvin@ootbdev.com>"
  ],
  "repository": {
    "url": "git@github.com:ootbdev/marqeta-core.git",
    "type": "git"
  },
  "author": "Alvin Lee <alvin@ootbdev.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "_jest": "jest",
    "_dev_env": "env-cmd -f ./config/environments/development.env",
    "prepublish": "babel -d lib src",
    "lint": "yarn eslint --ignore-path ./.gitignore ./src",
    "test": "yarn lint && yarn _jest",
    "test_no_lint": "yarn _jest",
    "test_ci": "yarn _jest --ci",
    "cli": "yarn _dev_env babel-node --config-file ./.babelrc"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/initialize.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      ".test.js"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-jest": "^25.4.0",
    "env-cmd": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "faker": "^4.1.0",
    "jest": "^25.3.0",
    "json-loader": "^0.5.7",
    "standard": "^14.3.3"
  },
  "dependencies": {
    "axios": "^0.19.2"
  }
}
